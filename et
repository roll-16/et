ex 6:
sudo apt-get update
sudo apt-get install -y snort
sudo apt install -y snort
git clone https://github.com/snort3/snort3.git
sudo snort -d -l /var/log/snort/ -h 192.168.0.102/24 -A console -c /etc/snort/snort.conf

-----------------------------------------------------------------------------

ex 7:
sudo apt update
sudo apt-get update
sudo apt-get install sqlmap
sudo snap install sqlmap
python3 -m pip show sqlmap
sudo pip3 install sqlmap
sudo apt install python3-pip
sudo apt-get install python3
sqlmap --url http://www.site.com/vuln.php?id=1
sqlmap -u http://testphp.vulnweb.com/listproducts.php?cat=1 --dbs

-----------------------------------------------------------------------------------------

ex 8:
sudo apt-get install netsniff-ng
sudo apt-get install -y netsniff-ng
sudo netsniff-ng

--------------------------------------------------------------------------------------------

ex 9:
@echo off
echo Detecting Viruses......
:start
cd C:\
IF EXIST swing.bat goto infected
IF NOT EXIST virus.bat goto clean
:infected
echo WARNING SWING.BAT VIRUS DETECTED!
del swing.bat
pause
goto start
:clean
echo your System is secure now and virus has been deleted!
pause
exit


%O|%O
